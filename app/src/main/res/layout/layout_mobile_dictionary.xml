    <TableRow android:layout_height="wrap_content"
        android:layout_width="match_parent">
        <LinearLayout
            android:background="@color/mobileLayoutButtonGroupBackground"
            android:padding="@dimen/mobileLayoutButtonGroupPadding"
            android:layout_height="match_parent"
            android:weightSum="1">

            <TextView
                android:id="@+id/sym_dictionary"
                style="?attr/buttonBarButtonStyle"
                android:layout_width="@dimen/mobileLayoutButtonWidth"
                android:layout_height="match_parent"
                android:layout_weight="1"
